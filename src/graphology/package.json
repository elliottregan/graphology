{
  "name": "graphology",
  "version": "0.26.0",
  "description": "A robust and multipurpose Graph object for JavaScript and TypeScript.",
  "type": "module",
  "main": "./dist/graphology.js",
  "module": "./dist/graphology.js",
  "browser": "./dist/graphology.umd.min.js",
  "types": "./dist/graphology.d.ts",
  "exports": {
    ".": {
      "types": "./dist/graphology.d.ts",
      "import": "./dist/graphology.js",
      "default": "./dist/graphology.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist specs",
    "build": "bun run build.ts",
    "prepare": "bun run build",
    "prepublishOnly": "bun test && bun run test:types && bun run build",
    "test": "bun test",
    "test:types": "tsc --noEmit"
  },
  "files": [
    "dist/*.js",
    "dist/*.mjs",
    "dist/*.ts",
    "dist/*.map"
  ],
  "keywords": [
    "graph",
    "graph theory",
    "directed",
    "undirected",
    "network",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphology/graphology.git"
  },
  "contributors": [
    {
      "name": "Alexis Jacomy",
      "url": "http://github.com/jacomyal"
    },
    {
      "name": "Benjamin Ooghe-Tabanou",
      "url": "http://github.com/boogheta"
    },
    {
      "name": "Guillaume Plique",
      "url": "http://github.com/Yomguithereal"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphology/graphology/issues"
  },
  "homepage": "https://github.com/graphology/graphology#readme",
  "dependencies": {
    "events": "^3.3.0"
  },
  "peerDependencies": {
    "graphology-types": ">=0.24.0"
  }
}
